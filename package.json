{
   "name": "server",
   "version": "0.0.0",
   "scripts": {
      "build:prod": "tsc --project tsconfig.prod.json",
      "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
      "deve": "NODE_ENV=development ts-node ./src",
      "dev": "nodemon --exec \"npm run deve\" --watch ./src --ext .ts",
      "lint": "eslint . --ext .ts || true",
      "start": "NODE_ENV=production node -r ./dist/config.js ./dist/src/index.js",
      "test": "NODE_ENV=test vitest",
      "type-check": "tsc --noEmit"
   },
   "engines": {
      "node": ">=16.0.0"
   },
   "dependencies": {
      "@sinclair/typebox": "^0.34.38",
      "ajv": "^8.17.1",
      "ajv-formats": "^3.0.1",
      "bcryptjs": "^3.0.2",
      "cloudinary": "^2.7.0",
      "cookie-parser": "^1.4.7",
      "cors": "^2.8.5",
      "dayjs": "^1.11.13",
      "dotenv": "^17.2.1",
      "express": "^5.1.0",
      "helmet": "^8.1.0",
      "inserturlparams": "^2.0.5",
      "jet-env": "^1.1.4",
      "jet-logger": "^2.0.1",
      "jet-paths": "^1.1.0",
      "jet-validators": "^1.4.3",
      "jsonfile": "^6.1.0",
      "jsonwebtoken": "^9.0.2",
      "module-alias": "^2.2.3",
      "mongoose": "^8.17.0",
      "morgan": "^1.10.1",
      "multer": "^2.0.2",
      "reflect-metadata": "^0.2.2",
      "slugify": "^1.6.6",
      "tsyringe": "^4.10.0"
   },
   "devDependencies": {
      "@swc/core": "^1.13.3",
      "@types/cookie-parser": "^1.4.9",
      "@types/cors": "^2.8.19",
      "@types/find": "^0.2.4",
      "@types/fs-extra": "^11.0.4",
      "@types/jest": "^30.0.0",
      "@types/jsonfile": "^6.1.4",
      "@types/jsonwebtoken": "^9.0.10",
      "@types/module-alias": "^2.0.4",
      "@types/morgan": "^1.9.10",
      "@types/multer": "^2.0.0",
      "@types/node": "^24.2.0",
      "@types/supertest": "^6.0.3",
      "find": "^0.3.0",
      "fs-extra": "^11.3.0",
      "jiti": "^2.5.1",
      "nodemon": "^3.1.10",
      "supertest": "^7.1.4",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.8.3",
      "vite-tsconfig-paths": "^5.1.4",
      "vitest": "^3.2.4"
   }
}
